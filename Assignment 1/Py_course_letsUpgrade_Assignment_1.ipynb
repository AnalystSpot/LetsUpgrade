    "# B. M. Suhaib\n",
    "###  Find Simple Interest(si) for a given Principle(p), time(t) and rate of interest(r)\n",
    "    simple_interest=(p*t*r)/100"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "adf51dd1",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter Principle:10000\n",
      "Time:2\n",
      "Rate of interest:3\n",
      "Simple Interest : 600.0\n"
     ]
    }
   ],
   "source": [
    "p=int(input('Enter Principle:'))\n",
    "t=int(input('Time:'))\n",
    "r=int(input('Rate of interest:'))\n",
    "si=(p*t*r)/100\n",
    "print(\"Simple Interest : {}\".format(si))"
   ]
  }
